runtime: nodejs16

automatic_scaling:
  max_instances: 1
  min_instances: 1

inbound_services:
- warmup

env_variables:
  TELEGRAM_BOT_TOKEN: ${TELEGRAM_BOT_TOKEN}
  TELEGRAM_CHAT_ID: ${TELEGRAM_CHAT_ID}
  DOMAIN: ${DOMAIN}
  PROTOCOL: ${PROTOCOL}
  TRUSTED_IP: ${TRUSTED_IP}
  AUGUST_API_KEY: ${AUGUST_API_KEY}
  AUGUST_INSTALLID: ${AUGUST_INSTALLID}
  AUGUST_PASSWORD: ${AUGUST_PASSWORD}
  AUGUST_ID_TYPE: ${AUGUST_ID_TYPE}
  AUGUST_ID: ${AUGUST_ID}
  LOCK_ID: ${LOCK_ID}
  
